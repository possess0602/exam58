sid <- 'S0102'
g_sql <- paste("SELECT s.degree
FROM segment_data s, site_data sd
WHERE (s.site_id = sd.id)
AND s.site_id = '",sid,"'
AND s.evaluation = 'P'", sep="")
good <- dbGetQuery(con, g_sql)
DegreeMax <- max(good$degree)
